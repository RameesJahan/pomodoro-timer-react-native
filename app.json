{
  "expo": {
    "name": "Pomodoro Timer",
    "slug": "pomodoro-timer",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#527e5c"
      },
      "package": "com.kaakkapps.pomodorotimer"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "5330e3c1-c28c-43a4-8dbe-38baf83c78be"
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "sounds": ["assets/sounds/beep_sound.mp3"]
        }
      ]
    ]
  }
}
